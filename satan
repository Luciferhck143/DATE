			plugin = random.choice([network,"AT&-T","T-Mobile"])
			random_seed = random.Random()
			adid = str(''.join(random_seed.choices(string.hexdigits, k=16))) 
			ua  = "[FBAN/FB4A;FBAV/"+str(random.randint(11,77))+".0.0."+str(random.randrange(9,49))+str(random.randint(11,77)) +";FBBV/"+str(random.randint(1111111,7777777))+";[FBAN/FB4A;FBAV/61.0.0.15.69;FBBV/20748118;FBDM/{density=4.0,width=1440,height=2560};FBLC/en_US;FBCR/3;FBMF/samsung;FBBD/samsung;FBPN/com.facebook.katana;FBDV/SM-G920F;FBSV/5.0.2;nullFBCA/armeabi-v7a:armeabi;]" 
