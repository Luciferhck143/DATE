'User-Agent': ua,
'Accept-Encoding': 'gzip, deflate',
'Accept': '/',
'Connection': 'keep-alive',
'POST': '/auth/login HTTP/2',
'Host': 'b-graph.facebook.com',
'Priority': 'u=3, i',
'Content-Type': 'application/x-www-form-urlencoded',
'X-Fb-Sim-Hni': '31002',
'X-Fb-Net-Hni': '31002',
